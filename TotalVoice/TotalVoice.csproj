<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>TotalVoice</PackageId>
    <Version>1.3.0</Version>
    <Authors>dilowagner</Authors>
    <Company>Totalvoice</Company>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Title>Client API TotalVoice</Title>
    <Description>Client .NET para API TotalVoice</Description>
    <RepositoryUrl>https://github.com/totalvoice/totalvoice-csharp</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.5.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
  </ItemGroup>

</Project>
